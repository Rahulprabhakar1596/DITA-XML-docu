            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">GET /tasks</cmd>
                <info class="- topic/itemgroup task/info ">Retrieve a list of tasks.</info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">GET /tasks</cmd>
                <info class="- topic/itemgroup task/info ">Retrieve details of a specific task.</info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">POST /tasks</cmd>
