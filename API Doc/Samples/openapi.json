{
  "openapi": "3.0.0",
  "info": {
    "title": "My Weatherstack API",
    "version": "1.0.0"
  },
  "paths": {
    "/current": {
      "get": {
        "summary": "Retrieve current weather data",
        "description": "Get the current weather conditions for a specific location.",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "temperature": {
                      "type": "number"
                    },
                    "humidity": {
                      "type": "number"
                    },
                    "wind_speed": {
                      "type": "number"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}